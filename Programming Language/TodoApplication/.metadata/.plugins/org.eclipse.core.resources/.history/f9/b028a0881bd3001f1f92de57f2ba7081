public class Main {

	public static void main(String args[]) throws ClassNotFoundException {
		Class.forName("com.mysql.cj.jdbc.Driver");
		try {
		Connection con=DriverManager.getConnection("","","");
		}
		catch(Exception e) {
			e.printStackTrace();
		}
		System.out.println("Driver ");
	}
}

