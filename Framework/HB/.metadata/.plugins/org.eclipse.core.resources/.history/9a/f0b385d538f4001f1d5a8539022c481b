package com.onetomany.AuthorBook.Unidirectional;

public class Book1 {
	private int b_id;
	private String name;
	private Author1 author;
	public Book1() {
		super();
		// TODO Auto-generated constructor stub
	}
	public Book1(String name, Author1 author) {
		super();
		this.name = name;
		this.author = author;
	}
	public int getB_id() {
		return b_id;
	}
	public void setB_id(int b_id) {
		this.b_id = b_id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public Author1 getAuthor() {
		return author;
	}
	public void setAuthor(Author1 author) {
		this.author = author;
	}
	@Override
	public String toString() {
		return "Book1 [b_id=" + b_id + ", name=" + name + ", author=" + author + "]";
	}
	
	

}
